{
  "name": "everything-demo",
  "version": "1.0.0",
  "description": "a electron demo",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
